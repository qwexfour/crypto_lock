<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="base.UsersEntity" table="Users" schema="public" catalog="postgres">
        <id name="id">
            <column name="ID" sql-type="integer"/>
        </id>
        <property name="surname">
            <column name="surname" sql-type="varchar"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar"/>
        </property>
        <property name="patronymic">
            <column name="patronymic" sql-type="varchar"/>
        </property>
        <property name="publicKey">
            <column name="public_key" sql-type="varchar"/>
        </property>
    </class>
</hibernate-mapping>